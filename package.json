{
  "name": "sync-to-tencent-cos",
  "version": "0.1.0",
  "private": true,
  "description": "sync to tencent cos",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhouxhere/sync-to-tencent-cos"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "cos-nodejs-sdk-v5": "^2.11.19"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "typescript": "^5.0.3"
  }
}
